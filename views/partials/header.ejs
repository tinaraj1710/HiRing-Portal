<body id="our-body">
	
	<% if(( error && error.length > 0 )  || (success && success.length >0)){ %>
	<div class="flashed-container">
	   <% if( error && error.length > 0 ){ %>
	     <div id="alertmess" class="alert alert-danger d-flex justify-content-between" role="alert">
            <p><%= error %></p>
			 <p>
				<input type="checkbox" id="flash-container" class="d-none">
                <label id="close-button-flash" for="flash-container">&times;</label> 
			 </p>
         </div>
       <% } %>
	
	   <% if(success && success.length > 0){ %>
	       <div id="alertmess" class="alert alert-success d-flex justify-content-between" role="alert">
             <p><%= success %></p>
			   <p>
				  <input type="checkbox" id="flash-container" class="d-none">
                  <label id="close-button-flash" for="flash-container">&times;</label>
			   </p> 
          </div>
       <% } %>
     </div>
	<% } %>
	
    <div class="screen-blocker main-loader-div">
    </div>  
	
	<div id="whole-loader" class="preloader-all-pages-whole main-loader-div">
		<svg class="loading-box">
            <circle class="loader" fill="none" stroke-width="3" stroke-linecap="round" cx="24" 
					cy="24" r="17"></circle>
        </svg>
    </div>
	
	
<!-- All resume update models -->

<!-- WORKEXP UPDATE MODAL -->
<div class="workexpsession" id="<%= workexps.length %>">
	<% var count = 0 %>
<% if(workexps.length > 0){ %>
  <% workexps.forEach(function(workexp){ %>
	<% count = count + 1 %>
	<div class="modal fade bd-<%= workexp._id %>-modal-lg" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true">
  <div class="modal-dialog modal-lg">
    <div id="modal-content" class="modal-content">
		<div class="preloader-all-pages main-loader-div">
		  <svg class="loading-box">
              <circle class="loader" fill="none" stroke-width="3" stroke-linecap="round" cx="24" 
					cy="24" r="17"></circle>
           </svg>
        </div>
		<h5 id="employer-send-offerletter-page" id="exampleModalLabel">Update Work Experience</h5>
				<button id="employer-send-offerletter-page-button" type="button" class="close" 
						data-dismiss="modal" aria-label="Close">
				  <span aria-hidden="true">&times;</span>
				</button>
		      <form class="submit-forms" id="send-offerletter-for-offerletter-page" action="/user/workexp/<%= workexp._id %>?_method=PUT" method="POST" enctype="multipart/form-data">
			      <div id="workexp-modal-body" class="modal-body">
				      <div id="first-form-row" class="form-row">
								<div class="form-group col-md-6">
								<label for="recipient-name" class="col-form-label">Title</label>
								<input type="text" class="form-control" id="recipient-name" name="role" value="<%= workexp.role %>">
							  </div>
							   <div class="form-group col-md-6">
								<label for="recipient-name" class="col-form-label">Company Name</label>
								<input type="text" class="form-control" id="recipient-name" name="companyname" value="<%= workexp.companyname %>" >
							  </div>
						  </div>	
						  <div id="second-form-row" class="form-row">
							  <div class="form-group col-md-6">
								<label for="worked-from" class="col-form-label">Worked from</label>
								<input type="date" class="form-control checkdatastart" id="updatedatefrom<%= count %>" name="workedfrom" value="<%= workexp.workedfrom %>">
							  </div>
							  <div class="form-group col-md-6">
								<label for="worked-to" class="col-form-label">Worked To</label>
								<input type="date" class="form-control checkdateend" id="updatedatecompl<%= count %>" name="workedto" value="<%= workexp.Workedto %>">
							  </div>
							</div>
							  <div id="form-check" class="form-check">
						        <label class="form-check-label" for="exampleCheckboxes">Currently Working On Their</label>
                                <input type="checkbox" class="form-check-input checkboxsession" id="exampleCheckboxes<%= count %>" name="currentworking" value="yes"><p>( If yes Then click on checkbox )</p>
                          </div>
							  <div id="forth-form-row" class="form-row">
								<div class="form-group col-md-6">
								 <label for="recipient-name" class="col-form-label">File Title</label>
								 <input type="text" class="form-control" id="recipient-name" name="filetitle" value="<%= workexp.filetitle %>">
							   </div>
					      </div>
					      <div id="forth-form-row" class="form-row">
							 <div id="input-file-of-attachment" class="col-md-12">
							   <button id="each-offerletter-button-accept-3" type="button" class="btn btn-sm btn-primary">
								  <label for="attach-file-session3" id="attachment-label-offerletter-page" class="col-form-label"> 
									  <img id="attchment-file-new-send-offerletter" src="/images/main-pages/white-attach-file.svg"                                                alt="attchment-image" > Attach file </label> </button> <span id="attachment-text3">No Files choosen, Yet.</span> 
								 <input type="file" class="d-none form-control" id="attach-file-session3" name="educationfiles" multiple required>
							 </div>
					     </div>
					   <div id="third-form-row" class="form-row text-area-div-session">
							   <div class="form-group col-md-6">
								 <label for="recipient-name" class="col-form-label">City</label>
								 <input type="text" class="form-control" id="recipient-name" name="city" value="<%= workexp.city %>">
							   </div>
							   <div class="form-group col-md-6">
								 <label for="recipient-name" class="col-form-label">Country</label>
								 <input type="text" class="form-control" id="recipient-name" name="country" value="<%= workexp.country %>">
							   </div>
							</div>

							  <div id="text-area-fleld" class="form-group">
								<label for="message-text" class="col-form-label">Description</label>
								<textarea class="form-control" id="message-text" rows="5" name="text"><%= workexp.text %></textarea>
							  </div>
				      <div class="send-offerletter-shortcut-div d-flex justify-content-center">
						  <button id="each-offerletter-button-accept-3" type="submit" class="btn btn-sm btn-primary">Save</button>
					  </div>
			     </div>
			    </form>
			</div>
	</div>
</div>
<%  }); } %>
</div>



